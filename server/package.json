{
  "name": "web-app-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node_modules/next/dist/bin/next start",
    "lint": "next lint",
    "prisma:migrate": "cross-env dotenv -e .env.local -- prisma migrate dev",
    "prisma:migrate:prod": "cp prisma/schema.sqlserver.prisma prisma/schema.prisma && cross-env dotenv -e .env.prod -- prisma migrate deploy && git checkout prisma/schema.prisma",
    "db:dev:prepare": "node scripts/copy-file.js prisma/schema.dev.prisma prisma/schema.prisma",
    "db:dev:save": "node scripts/copy-file.js prisma/schema.prisma prisma/schema.dev.prisma",
    "db:dev:migrate:prepare": "node scripts/prepare-migrations.js dev",
    "db:dev:migrate:save": "node scripts/save-migrations.js dev",
    "db:dev:migrate": "pnpm db:dev:prepare && pnpm db:dev:migrate:prepare && cross-env dotenv -e .env.local -- prisma migrate dev && pnpm db:dev:migrate:save",
    "db:dev:deploy": "pnpm db:dev:prepare && pnpm db:dev:migrate:prepare && cross-env dotenv -e .env.local -- prisma migrate deploy && pnpm db:dev:migrate:save",
    "db:dev:reset": "pnpm db:dev:prepare && pnpm db:dev:migrate:prepare && cross-env dotenv -e .env.local -- prisma migrate reset --force && pnpm db:dev:migrate:save",
    "db:prod:prepare": "node scripts/copy-file.js prisma/schema.prod.prisma prisma/schema.prisma",
    "db:prod:save": "node scripts/copy-file.js prisma/schema.prisma prisma/schema.prod.prisma",
    "db:prod:migrate:prepare": "node scripts/prepare-migrations.js prod",
    "db:prod:migrate:save": "node scripts/save-migrations.js prod",
    "db:prod:migrate": "pnpm db:prod:prepare && pnpm db:prod:migrate:prepare && cross-env dotenv -e .env.prod -- prisma migrate dev && pnpm db:prod:migrate:save",
    "db:prod:deploy": "pnpm db:prod:prepare && pnpm db:prod:migrate:prepare && cross-env dotenv -e .env.prod -- prisma migrate deploy && pnpm db:prod:migrate:save",
    "db:prod:reset": "pnpm db:prod:prepare && pnpm db:prod:migrate:prepare && cross-env dotenv -e .env.prod -- prisma migrate reset --force && pnpm db:prod:migrate:save",
    "db:migrate:both": "pnpm db:dev:migrate && pnpm db:prod:migrate",
    "db:switch:prod": "node scripts/switch-db.js prod",
    "db:switch:dev": "node scripts/switch-db.js dev"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/uuid": "^10.0.0",
    "@use-gesture/react": "^10.3.1",
    "applicationinsights": "^3.4.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.462.0",
    "next": "15.0.3",
    "next-auth": "^4.24.11",
    "next-intl": "^3.26.5",
    "openai": "^5.3.0",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "ignore-loader": "^0.1.2",
    "postcss": "^8",
    "prisma": "^6.0.0",
    "rimraf": "^5.0.10",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "packageManager": "pnpm@9.4.0"
}
